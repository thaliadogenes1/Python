
inicio = int(input("Digite o valor inicial: "))
fim = int(input("Digite o valor final: "))
passo = int(input("Digite o passo (ex: 1 para contar de 1 em 1): "))

if passo == 0:
    print("O passo n√£o pode ser zero.")
else:
    print("\nContando...")

    for numero in range(inicio, fim + 1):
        print(numero)
